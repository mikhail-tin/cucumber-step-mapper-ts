{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "extensionHost", 
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [ "--extensionDevelopmentPath=${workspaceRoot}" ],
            "sourceMaps": true,
            "outFiles": [ "${workspaceRoot}/out/src/**/*.js" ],
            "preLaunchTask": "npm: compile"
        }
    ]
}
